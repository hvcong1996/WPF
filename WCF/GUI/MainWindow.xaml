<Window x:Class="GUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="200" Width="280">
    <Grid>
        <Button Width="100" Height="50" Click="Update_Click" Margin="20,30,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">Update</Button>
        <Button Width="100" Height="50" Click="UpdateSetting_Click" Margin="150,30,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">Update Setting</Button>
        <Button Width="100" Height="50" Click="Activate_Click" Margin="20,100,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">Activate</Button>
        <Button Width="100" Height="50" Click="Deactivate_Click" Margin="150,100,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">Deactivate</Button>
    </Grid>
</Window>
